<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/actionbar_layout"
        android:id="@+id/include" />

    <ImageView
        android:id="@+id/imageView4"
        android:background="@drawable/login_icon"
        android:layout_width="@dimen/x178"
        android:layout_height="@dimen/y178"
        android:layout_below="@+id/include"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp" />
    <TextView
        android:id="@+id/textview_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="HMS手机版"
        android:textColor="#000"
        android:textSize="@dimen/y40"
        android:layout_marginTop="@dimen/y30"
        android:layout_below="@+id/imageView4"
        android:layout_centerHorizontal="true" />
    <TextView
        android:id="@+id/textview_NowVersion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="当前版本：1.190926.0.1"
        android:textColor="#000"
        android:textSize="@dimen/y30"
        android:layout_marginTop="@dimen/y22"
        android:layout_below="@+id/textview_1"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/imageView_Update_Btn"
        android:layout_width="@dimen/x500"
        android:layout_height="@dimen/y72"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/update" />
</RelativeLayout>